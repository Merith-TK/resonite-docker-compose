{
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Debug" ],
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "System": "Information",
                "Microsoft": "Information",
                "System.Net.Http.HttpClient": "Warning"
            }
        }
    },
    "Headless": {
        "resonitePath": "/data/resonite",
        "assetCleanupInterval": "06:00:00",
        "maxAssetAge": "07:00:00:00"
    }
}